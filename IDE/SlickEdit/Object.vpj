<!DOCTYPE Project SYSTEM "http://www.slickedit.com/dtd/vse/10.0/vpj.dtd">
<Project
    WorkingDir="."
    Version="10.0"
    VendorName="SlickEdit"
    TemplateName="GNU Ada Compiler">
    <Files>
        <Folder
            Name="Source Files"
            Filters="*.ada;*.adb;*.c;*.cpp"
            GUID="{EB2EB1C2-114F-4FBE-BA04-4E65F9EBE7F8}"/>
        <Folder
            Name="Header Files"
            Filters="*.h;*.ads"
            GUID="{27C4EB76-D765-46C5-94F0-D459FCFBE6B5}"/>
        <Folder
            Name="Resource Files"
